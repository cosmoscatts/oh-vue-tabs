{
  "name": "@cosmoscatts/oh-vue-tabs",
  "private": false,
  "description": "tab view component of two styles, default & chrome.",
  "bugs": {
    "url": "https://github.com/cosmoscatts/oh-vue-tabs/issues"
  },
  "keywords": [
    "vue3",
    "chrome tab"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --port 3333 --open",
    "build": "vue-tsc --noEmit && vite build",
    "build:netlify": "cross-env VITE_IS_NETLIFY=1 vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@unocss/reset": "^0.51.12",
    "colord": "^2.9.3",
    "vue": "^3.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@iconify/json": "^2.2.64",
    "@types/node": "20.1.3",
    "@unocss/preset-icons": "^0.51.12",
    "@unocss/preset-wind": "^0.51.12",
    "@vitejs/plugin-vue": "^4.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "typescript": "^5.0.4",
    "unocss": "^0.51.12",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.6.4"
  }
