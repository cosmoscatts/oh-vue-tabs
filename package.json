{
  "name": "@cosmoscatts/oh-vue-tabs",
  "private": false,
  "description": "tab view component of two styles, default & chrome.",
  "bugs": {
    "url": "https://github.com/cosmoscatts/oh-vue-tabs/issues"
  },
  "keywords": [
    "vue3",
    "chrome tab"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --port 3333 --open",
    "build": "vue-tsc --noEmit && vite build",
    "build:netlify": "cross-env VITE_IS_NETLIFY=1 vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@unocss/reset": "^0.49.4",
    "colord": "^2.9.3",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.1",
    "@iconify/json": "^2.2.15",
    "@types/node": "18.11.18",
    "@unocss/preset-icons": "^0.49.4",
    "@unocss/preset-wind": "^0.49.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "typescript": "^4.9.5",
    "unocss": "^0.49.4",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.2",
    "vue-tsc": "^1.0.24"
  }
}
